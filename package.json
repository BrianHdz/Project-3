    "node-fetch": "^2.6.0",
    "react-grid-layout": "^0.18.3",
    "react-icons": "^3.10.0",
    "react-iframe": "^1.8.0",
    "react-media-visualizer": "^1.1.1",
    "react-redux": "^7.2.0",
